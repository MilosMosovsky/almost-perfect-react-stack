  "devDependencies": {
    "@types/jest": "^23.3.10",
    "@types/react": "^16.7.18",
    "jest": "^23.6.0",
    "npm-watch": "^0.5.0",
    "parcel": "^1.11.0",
    "react-testing-library": "^5.4.2",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.2.2"
  },
  "dependencies": {
    "prop-types": "^15.5.7-alpha.1",
    "react": "^16.7.0-alpha.2",
    "react-dom": "^16.7.0-alpha.2",
    "react-router5": "^7.0.2",
    "router5": "^7.0.1",
    "router5-plugin-browser": "^7.0.1",
    "router5-plugin-logger": "^7.0.1",
    "styled-components": "^4.1.3"
  }
